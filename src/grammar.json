{
  "name": "codepatterns",
  "rules": {
    "query": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_token"
      }
    },
    "_token": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "literal"
        },
        {
          "type": "SYMBOL",
          "name": "captureLabel"
        },
        {
          "type": "SYMBOL",
          "name": "regex"
        },
        {
          "type": "SYMBOL",
          "name": "tsq"
        },
        {
          "type": "SYMBOL",
          "name": "lineQuantifier"
        },
        {
          "type": "SYMBOL",
          "name": "replaceStart"
        },
        {
          "type": "SYMBOL",
          "name": "replaceEnd"
        }
      ]
    },
    "literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_escape"
        },
        {
          "type": "PATTERN",
          "value": "[^\\\\\\/\\[\\]@\\(]+"
        }
      ]
    },
    "_escape": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\\"
        },
        {
          "type": "PATTERN",
          "value": "."
        }
      ]
    },
    "captureLabel": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "PATTERN",
          "value": "[a-zA-Z]+"
        }
      ]
    },
    "replaceStart": {
      "type": "STRING",
      "value": "["
    },
    "replaceEnd": {
      "type": "STRING",
      "value": "]"
    },
    "_whitespace": {
      "type": "PATTERN",
      "value": "[ \\t]+"
    },
    "_newlines": {
      "type": "PATTERN",
      "value": "[\\r\\n]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "tsq"
    },
    {
      "type": "SYMBOL",
      "name": "lineQuantifier"
    },
    {
      "type": "SYMBOL",
      "name": "regex"
    },
    {
      "type": "SYMBOL",
      "name": "_errorSentinel"
    }
  ],
  "inline": [],
  "supertypes": []
}

